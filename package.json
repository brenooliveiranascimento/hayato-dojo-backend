{
  "name": "karate-championship-api",
  "version": "1.0.0",
  "description": "API para cadastro de atletas de karatÃª",
  "main": "src/server.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm -- migration:generate",
    "migration:run": "yarn typeorm migration:run -- -d src/database/typeorm/data-source.ts",
    "migration:revert": "yarn typeorm migration:revert -- -d src/database/typeorm/data-source.ts"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^17.0.1",
    "fastify": "^4.24.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.4",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/node": "^20.8.0",
    "@types/pg": "^8.10.7",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
